{
    "name": "silverstripe/installer",
    "type": "silverstripe-project",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^7.1 || ^8",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.7.0@stable",
        "silverstripe-themes/simple": "~3.2.0",
        "silverstripe/login-forms": "4.3.0@stable",
        "dnadesign/silverstripe-elemental": "^4",
        "silverstripe/elemental-bannerblock": "^2.0",
        "silverstripe/elemental-fileblock": "^2.0",
        "gorriecoe/silverstripe-link": "^1.3",
        "gorriecoe/silverstripe-linkfield": "^1.0",
        "unclecheese/display-logic": "^2.0",
        "silverstripe/googlesitemaps": "^2.1",
        "dnadesign/silverstripe-elemental-userforms": "^3.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2",
        "undefinedoffset/silverstripe-nocaptcha": "^2.1"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ],
        "expose": [
            "themes/site/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
